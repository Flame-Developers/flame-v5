{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-restricted-globals": "off",
        "no-restricted-syntax": "off",
        "no-trailing-spaces": "off",
        "no-console": "off"
    }
}
